{
    "diagnostics": {
        "disable": [
            "inject-field",
            "close-non-object",
            "incomplete-signature-doc",
            "missing-global-doc",
            "missing-local-export-doc",
            "duplicate-set-field",
            "undefined-global",
            "undefined-field"
        ],
        "groupFileStatus": {
            "ambiguity": "Any",
            "await": "Any",
            "duplicate": "Any",
            "global": "Any",
            "luadoc": "Any",
            "redefined": "Any",
            "strict": "Any",
            "type-check": "Any",
            "unbalanced": "Any",
            "unused": "Any"
        },
        "ignoredFiles": "Opened",
        "libraryFiles": "Opened"
    },
    "runtime": {
        "version": "Lua 5.1",
        "path": [
            "script/?.lua",
            "script/?/init.lua",
            "test/?.lua",
            "test/?/init.lua",
            "script/meta/?.lua"
        ],
        "pathStrict": true
    },
    "workspace": {
        "maxPreload": 1600,
        "preloadFileSize": 1000,
        "ignoreDir": [
            "/Libs/",
            "/libs/",
            "/.vscode"
        ],
        "checkThirdParty": false
    },
    "typeFormat": {
        "config": {
            "format_line": "false"
        }
    },
    "type": {
        "castNumberToInteger": false
    },
    "doc": {
        "privateName": [
            "_*"
        ]
    },
